                                    <script src="../../Recursos/js/menu.js" type="text/javascript"></script>
                                    <script src="../../Recursos/js/ajax/verifica_sesion.js" type="text/javascript"></script>
                                    <script src="../../Recursos/css/menu/script.js" type="text/javascript"></script>
                                    <link href="../../Recursos/css/menu/styles.css" rel="stylesheet" />
                                    <input type="text" class="ocultar" id="permisos" value="<% Response.Write(Session["permisos"]);%>" />
                                    <div id='cssmenu'>
                                        <ul>
                                            <li id="liI"><a href='<% Response.Write(Page.ResolveUrl("~/Vista/general/inicio.aspx")); %>'>
                                                <span>Inicio</span></a>
                                                <ul>
                                                    <li title="Inicio" id="liI.1"><a href="<% Response.Write(Page.ResolveUrl("~/vista/administracion/inicio.aspx")); %>">
                                                        <span>inicio</span></a> </li>
                                                </ul>
                                            </li>
                                            <li id="li0" class='active has-sub'><a href='javascript:void(0)'><span>Administración</span></a>
                                                <ul>
                                                    <li title="Roles" id="li0.1"><a href="<% Response.Write(Page.ResolveUrl("~/vista/administracion/rol.aspx")); %>">
                                                        <span>Roles</span></a> </li>
                                                    <li title="Usuarios" id="li0.6"><a href="<% Response.Write(Page.ResolveUrl("~/vista/administracion/usuario.aspx")); %>">
                                                        <span>Usuarios</span></a> </li>
                                                    <li title="Usuarios Conectados" id="li0.8"><a href="<% Response.Write(Page.ResolveUrl("~/vista/administracion/usuarios_conectados.aspx")); %>">
                                                        <span>Usuarios Conectados</span></a> </li>
                                                </ul>
                                            </li>
                                            <li id="li2" class='active has-sub'><a href='javascript:void(0)'><span>Solicitud Prestamo</span></a>
                                                <ul>
                                                    <li title="SolicitudPrestamo" id="li2.A"><a href="<% Response.Write(Page.ResolveUrl("~/vista/SolicitudPrestamo/SolicitudPrestamo.aspx")); %>">
                                                        <span>Solicitud Prestamo</span></a> </li>
                                                </ul>
                                            </li>
                                            <li id="limc" class='active has-sub' style="display: block;"><a href="javascript:void(0)">
                                                <span>Mi cuenta</span></a>
                                                <ul>
                                                    <li id="licc" style="display: block;"><a href="<% Response.Write(Page.ResolveUrl("~/vista/general/cambio_clave.aspx")); %>">
                                                        <span>Cambiar Clave</span></a></li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </td>
                            <td style="vertical-align: initial">
                                <a onclick="muestraDiv()" href='#'>
                                    <label id="lblMsj" style="display: none">Ocultar</label><img id="imagenIzq" src="../../Recursos/imagenes/paginaMaestra/izquierda.png" width="18" /><img style="display: none" id="imagenDer" src="../../Recursos/imagenes/paginaMaestra/derecha.png" width="18" /></a>
                            </td>
                        </tr>
                    </table>
                </div>
            </td>
            <td class="alinearArriba">
                <% }
               else
               {  %>
                <td colspan="2" class="alinearArriba">
                    <%} %>
                    <div>
                        <table cellspacing="0" cellpadding="0" border="0" id="divContenidoForm">
                            <tr>
                                <% if ((Session["nom_usuario"]) == null)
                                   {
                                %>
                                <td style="width: 280px;" class="barraSesionUsuario"></td>
                                <%} %>
                                <td style="width: 1251px;" class="barraSesionSalir alinearArriba">
                                    <div>
                                        <asp:ContentPlaceHolder ID="contenidoSistema" runat="server">
                                        </asp:ContentPlaceHolder>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>
                </td>
            </td>
        </tr>
        <tr>
            <td colspan="10">
                <div style="width: 100%; background-color: #016678; height: 100px;">
                    <table class="centrarPie" border="0" cellpadding="0">
                    </table>
                </div>
            </td>
        </tr>
    </table>
    <div id="ventana" class="ventanaConfirmacion ocultar">
        <table>
            <tr>
                <td class="encabezado">
                    <span>Mensaje</span>
                </td>
            </tr>
            <tr>
                <td>
                    <label id="msjVentana" class="msjVentana">
                    </label>
                </td>
            </tr>
        </table>
    </div>
    <div id="ventanaProg" class="ventanaConfirmacion ocultar">
        <table>
            <tr>
                <td class="encabezado">
                    <span>Mensaje</span>
                </td>
            </tr>
            <tr>
                <td>
                    <label id="msjVentanaProg" class="msjVentana">
                    </label>
                </td>
            </tr>
            <tr>
                <td align="center">
                    <div id="divProgreso">
                        <img src="../../Recursos/imagenes/generales/progress_bar.gif" title="Progreso" alt="Procesando..." />
                    </div>
                </td>
            </tr>
        </table>
    </div>
    <!--Ventana con Mensaje para Confirmar Eliminación-->
    <div style="display: none;">
        <div id="confirmaEliminar" class="fancyEliminar">
            <table class="centrar">
                <tr>
                    <td class="encabezado">
                        <span>Mensaje</span>
                    </td>
                </tr>
                <tr>
                    <td>
                        <span>&#191;CONFIRMA QUE DESEA ELIMINAR ESTA INFORMACI&Oacute;N &#63;</span>
                    </td>
                </tr>
                <tr>
                    <td align="center">
                        <table>
                            <tr>
                                <td align="center">
                                    <span class="leyenda">Aceptar</span><br />
                                    <img src="../../Recursos/imagenes/administracion/aceptar.png" onclick="eliminar()"
                                        alt="Aceptar" class="imgAdmin" />
                                </td>
                                <td align="center">
                                    <span class="leyenda">Cancelar</span><br />
                                    <img src="../../Recursos/imagenes/administracion/cancelar.png" onclick="cerrarVentanaEmergente()"
                                        alt="Cancelar" class="imgAdmin" />
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</body>
</html>

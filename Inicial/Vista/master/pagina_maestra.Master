﻿<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="pagina_maestra.master.cs"
    Inherits="Inicial.Vista.master.pagina_maestra" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1" runat="server">
    <meta http-equiv="pragma" content="no-cache" />
    <title>
        <asp:ContentPlaceHolder ID="titulo" runat="server">
        </asp:ContentPlaceHolder>
    </title>
    <link href="../../Recursos/css/general.css" rel="stylesheet" type="text/css" />
    <link href="../../Recursos/css/paginaMaestra.css" rel="stylesheet" type="text/css" />
    <script src="../../Recursos/js/jquery.js" type="text/javascript"></script>
    <script src="../../Recursos/js/jQuery_adicional.js" type="text/javascript"></script>
    <script src="../../Recursos/js/validarCampos.js" type="text/javascript"></script>
    <script src="../../Recursos/js/ajax/paginaMaestra.js" type="text/javascript"></script>
    <script src="../../Recursos/js/ajax/cerrarSesion.js" type="text/javascript"></script>
    <%-------------------------------------------------------------------------%>
    <script src="../../Recursos/js/cerrarSesion.js" type="text/javascript"></script>
    <%-------------------------------------------------------------------------%>
    <link href="../../Recursos/css/cerrarSesion/styles.css" rel="stylesheet" type="text/css" />
    <%-------------------------------------------------------------------------%>
    <% 
        if ((Session["nom_usuario"]) != null)
        {
    %>
    <!-- Estilos para el menu -->
    <link href="../../Recursos/css/menu.css" rel="stylesheet" type="text/css" />
    <% }
        else
        { %>
    <!-- Estilos para el formulario de login -->
    <link href="../../Recursos/css/login.css" rel="stylesheet" type="text/css" />
    <% } %>
</head>
<body>
    <table id="tbContenedor" border="0" cellpadding="0" cellspacing="0" style="width: 1030px;">
        <tr>
            <td colspan="2">
                <table class="tbBanner" border="0" cellpadding="0" cellspacing="0">
                    <%--   <tr>
                        <td>
                            <img style="width: 100%; height: 85px;" src="../../Recursos/imagenes/paginaMaestra/Head.jpg" />
                        </td>
                    </tr>--%>
                    <tr>
                        <td class="bannerproducto">
                            <%if (Session["imagen_empresa"] == null)
                              {  %>

                            <img style="width: 100%; height: 85px;" src="../../Recursos/imagenes/paginaMaestra/Head.jpg" />
                            <%--<img style="width: 100%; height: 85px;" src="../../Recursos/imagenes/paginaMaestra/headAzul.jpg" />--%>
                            <%}
                              else
                              {  %><img style="width: 100%; height: 85px;" src="../../Recursos/imagenes/paginaMaestra/Head.jpg" />
                            <%}%>
                        </td>

                                <table width="100%" border="0" cellpadding="0" cellspacing="0">
                                    <tr>
                                        <td class="barraSesionEmpresa" style="width: 22%; text-align: left">
                                            <%
                                  if (Session["director"] == "1")
                                  {
                                      Response.Write("Empresa Actual: " + Session["nombre_empresa"]);
                                  }
